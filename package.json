{
  "name": "matpower-case-viewer",
  "version": "0.0.1",
  "scripts": {
    "wasm": "rm -rf wasm_matpower/pkg && wasm-pack build ./wasm_matpower --release --target web",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "deploy": "sh scripts/deploy.sh"
  },
  "devDependencies": {
    "@iconify/svelte": "^2.1.0",
    "@savvywombat/tailwindcss-grid-areas": "^2.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^1.0.13",
    "@types/cookie": "^0.4.1",
    "@types/d3": "^7.1.0",
    "@types/tailwindcss": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "autoprefixer": "^10.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.43.4",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-color-picker": "^1.0.7",
    "svelte-infinite-loading": "^1.3.8",
    "svelte-infinite-scroll": "^2.0.0",
    "svelte-inline-svg": "^1.0.8",
    "svelte-preprocess": "^4.9.8",
    "svelte2tsx": "^0.4.12",
    "tailwindcss": "^3.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "vite-plugin-comlink": "^2.1.0",
    "vite-plugin-wasm-pack": "^0.1.9",
    "vite-plugin-worker": "^1.0.5"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.0",
    "@lukeed/uuid": "^2.0.0",
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.6",
    "comlink": "^4.3.1",
    "cookie": "^0.4.1",
    "d3": "^7.2.1"
  }
}
