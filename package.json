    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@parcel/transformer-vue": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@vue/component-compiler-utils": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "execa": "latest",
    "parcel": "^2.0.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.0.8",
    "typescript": "^4.4.4",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.29.8"
